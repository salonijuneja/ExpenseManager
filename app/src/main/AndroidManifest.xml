<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.expensemanager" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Calc"
            android:label="@string/title_activity_calc" >
        </activity>
        <activity
            android:name=".WeekActivity"
            android:label="@string/title_activity_week" >
        </activity>
        <activity
            android:name=".Income"
            android:label="@string/title_activity_income" >
        </activity>
        <activity
            android:name=".Expense"
            android:label="@string/title_activity_expense" >
        </activity>
        <activity
            android:name=".YearActivity"
            android:label="@string/title_activity_year" >
        </activity>
        <activity
            android:name=".DayActivity"
            android:label="@string/title_activity_day" >
        </activity>
    </application>

</manifest>
